{
  "name": "boilerplate-js",
  "version": "1.0.0",
  "description": "Boilerplate for JavaScript projects with linter and tests",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "npx eslint src/*.js",
    "lint:fix": "npx eslint --fix src/*.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:all": "npm run lint && npm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "jest": "25.1.0"
  }
}
