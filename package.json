{
  "name": "boilerplate-js",
  "version": "1.0.0",
  "description": "Boilerplate for JavaScript projects with linter and tests",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "npx eslint src/*.js",
    "lint:fix": "npx eslint --fix src/*.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:all": "npm run lint && npm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "jest": "^24.9.0"
  }
}
